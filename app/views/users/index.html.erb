<h2> User Index </h2>



<table class= "table">
  
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First_name</th>
      <th scope="col">Last_name</th>
      <th scope="col">Email</th>
      <th scope="col">Category</th>
      <th scope="col"></th>

    </tr>
  </thead>
  <tbody>
     <%@users.each do |user| %>
      <tr>
        <td><%= user.id%></td>
        <td><%= user.first_name%></td>
        <td><%= user.last_name%></td>
        <td><%= user.email%></td>
        <td><%= user.category%></td>
        <td><%= link_to "show data", user_path(user.id), class: "btn btn-primary"%></td>
      </tr>

    <%end%>
    
  </tbody>
     
</table>