<h2> Organization Index </h2>

<table class= "table">
  
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Address</th>
      <th scope="col">Head_Quarter</th>
      <th scope="col"></th>

    </tr>
  </thead>
  <tbody>
     <%@organizations.each do |organization| %>
      <tr>
        <td><%= organization.id%></td>
        <td><%= organization.name%></td>
        <td><%= organization.address%></td>
        <td><%= organization.head_quarter%></td>
        <td><%= link_to "show data", organization_path(organization.id), class: "btn btn-primary"%></td>
      </tr>

    <%end%>
    
  </tbody>
     
</table>